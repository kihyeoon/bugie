    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^7.16.0"
  },
  "engines": {
    "node": ">=18"
